{
  "expo": {
    "name": "Recycle-Capstone",
    "description": "A very interesting project.",
    "slug": "Recycle-Capstone",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/app_logo.png",
    "splash": {
      "image": "./images/app_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.teamproton.recycleit",
      "supportsTablet": true,
      "infoPlist": { "NSLocationWhenInUseUsageDescription": "This app uses your location for the map." }
    },
    "android": {
      "package": "com.teamproton.recycleit"
    }
  }
}